<thead>
    <tr>
        <th>Id</th>
        <th>Nom</th>
        <th>Marque</th>
        <th>Annee</th>
        <th>Prix</th>
        <th>Couleur</th>
        <th>Actions</th>
    </tr>
</thead>
<tbody>
{% for moto in motos %}
    <tr>
        <td>{{ moto.id }}</td>
        <td>{{ moto.nom }}</td>
        <td>{{ moto.marque }}</td>
        <td>{{ moto.annee }}</td>
        <td>{{ moto.prix }}</td>
        <td>{{ moto.couleur }}</td>
        <td>
            <a href="{{ path('app_moto_show', {'id': moto.id}) }}">show</a>
            <a href="{{ path('app_moto_edit', {'id': moto.id}) }}">edit</a>
        </td>
    </tr>
{% endfor %}
</tbody>
